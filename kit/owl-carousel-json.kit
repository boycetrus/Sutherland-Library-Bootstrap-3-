<!DOCTYPE html>
<html>
  <head>
    <title>Library - Sutherland Shire Council</title>
    <!-- Bootstrap -->
    <!-- @import components/_head-content.htm -->
  </head>

  <body>

    <div class="container-fluid page-wrap">

      <!-- @import components/_header.htm -->

      <!-- @import components/_navbar.htm -->

      <!-- @import components/_breadcrumbs.htm -->

      <script type="text/x-handlebars-template" id="bookTemplate">
      	{{#each data.feed.entry}}
          <article class="book item">
            <a href="http://encore.sutherlandshire.nsw.gov.au/iii/encore/record/C__R{{gsx$bib.$t}}">
              <img src="{{gsx$cover.$t}}" alt="{{gsx$title.$t}}" />
            </a>
            <div class="book-detail sr-only">
              <h4>{{gsx$title.$t}}</h4>
              <p>{{{gsx$review.$t}}}</p>
              <p><a class="btn btn-success" target="_blank" href="http://encore.sutherlandshire.nsw.gov.au/iii/encore/record/C__R{{gsx$bib.$t}}">Get it!</a></p>
            </div>
          </article>
        {{/each}}
      </script>

      <script type="text/javascript">
        var gDocId = '1emCiiEC9NIWPwG1Fb1Tgv3qKkE2nfzwQp8lD332AhnM';
      </script>

      <section id="main" class="main row" role="main">

          <div id="content" class="col-xs-12 content">

            <h1>Book List Carousel</h1>
            <p class="lead">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Fusce ornare dictum enim, et dapibus est semper ac. Nunc sit amet fringilla felis, eu feugiat augue.</p>

            <div id="bookCarousel" class="book-carousel owl-carousel">
            </div>
            <div id="bookData" class="book-data">
              <button type="button" class="close" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
              <div class="info"></div>
            </div>

          </div><!-- /content -->


        </div><!-- /row -->

      </section><!--/main-->
    </div><!--/container-->

    <!-- @import components/_social.htm -->

    <!-- @import components/_footer.htm -->

  <script src="js/production/global-min.js"></script>
  <script src="js/production/json-carousel-min.js"></script>

  </body>
</html>
